  },
  "homepage": "https://github.com/rakshans1/docstash#readme",
  "dependencies": {
    "async": "^2.0.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "cors": "^2.7.1",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "express-status-monitor": "0.0.9",
    "jwt-simple": "^0.5.0",
    "mega": "^0.2.0",
    "mongoose": "^4.5.8",
    "morgan": "^1.7.0",
